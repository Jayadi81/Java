public class Perulangan {

  
    public static void main(String[] args) {
        //for Loop
       int i = 1; 
       while (i <= 10) {
           System.out.println("Telah berputar "+i+" kali");
           i++;
       }
       
       // Do-while loop
       int x = 6;
       do {
           System.out.println(x);
           x++;
       } while (x <= 5);    
    }
}
